<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zkhy.webrtc.android">

    <application
        android:name=".ARtcApp"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".LoginDemoActivity" />

        <activity
            android:name=".ARtcDemoActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity" />
        <activity
            android:name="com.dds.webrtclib.ui.plugin.VoiLoadingActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Webrtc.AppTheme.NoActionBar" />
        <activity
            android:name=".HomeDemoActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Webrtc.AppTheme.NoActionBar" />
    </application>

</manifest>